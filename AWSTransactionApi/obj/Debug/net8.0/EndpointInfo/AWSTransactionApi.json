          }
        },
        "additionalProperties": false
      },
      "CreateCardMessage": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "nullable": true
          },
          "request": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PayCreditRequest": {
        "type": "object",
        "properties": {
          "merchant": {
            "type": "string",
            "nullable": true
          },
          "amount": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "PurchaseRequest": {
        "type": "object",
        "properties": {
          "merchant": {
            "type": "string",
            "nullable": true
          },
          "cardId": {
            "type": "string",
            "nullable": true
          },
          "amount": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "SaveBalanceRequest": {
        "type": "object",
        "properties": {
          "merchant": {
            "type": "string",
            "nullable": true
          },
          "amount": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    }
  }
