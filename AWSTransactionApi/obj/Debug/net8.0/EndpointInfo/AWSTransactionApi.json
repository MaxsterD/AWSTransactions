{
  "openapi": "3.0.1",
  "info": {
    "title": "AWSTransactionApi",
    "version": "1.0"
  },
  "paths": {
    "/Transaction/card/create": {
      "post": {
        "tags": [
          "Card"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateCardMessage"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateCardMessage"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateCardMessage"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Transaction/card/activate": {
      "post": {
        "tags": [
          "Card"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ActivateCardRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ActivateCardRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ActivateCardRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/Transaction/transactions/purchase": {
      "post": {
        "tags": [
          "Card"
